{
    "name": "Archaeology Collection Tracker",
    "type": "app",
    "version": "1.0.2",
    "author": "aiLMJC",
    "description": "Tracks archaeology collection progress through interface scanning",
    "icon": "icon.png",
    "repo": "",
    "depends": [],
    "install": {
        "type": "folder",
        "files": [
            "index.html",
            "app.js",
            "styles.css",
            "metadata.json",
            "alt1.min.js"
        ]
    },
    "runtime": {
        "main": "index.html",
        "permissions": [
            "overlay",
            "screen",
            "pixel"
        ],
        "hotkey": "alt+1",
        "scaling": 1.0
    },
    "target": {
        "game": "runescape",
        "skill": "archaeology"
    }
}
